<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta content="width=device-width, initial-scale=1.0" name="viewport" />
        <meta http-equiv="Content-type" content="text/html; charset=utf-8">
        <meta content="" name="description" />
        <meta content="" name="author" />
        <link href="{{ asset('static/plugins/bootstrap/css/bootstrap.min.css')}}" rel="stylesheet" type="text/css">
        {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body class="page-header-fixed page-sidebar-closed-hide-logo">
        <!-- BEGIN HEADER -->
        {{include ('admin/default/header.html.twig') }}
        <!-- END HEADER -->
        <!-- BEGIN CONTAINER -->
        <div class="page-container">
            <!-- BEGIN SIDEBAR -->
            <div class="page-sidebar-wrapper">
            {{include ('admin/default/menus.html.twig') }}
            </div>
            <!-- END SIDEBAR -->
            <!-- BEGIN CONTENT -->
            <div class="page-content-wrapper">
                <div class="page-content">
                {% block body %}{% endblock %}
                </div>
            </div>
        </div>
        <script src="{{ asset('static/require.js') }}"></script>
        <script src="{{ asset('static/config.js') }}"></script>
        <script src="{{ asset('static/plugins/jquery.min.js') }}" type="text/javascript"></script>
        <script src="{{ asset('static/plugins/bootstrap/js/bootstrap.min.js') }}" type="text/javascript"></script>
        <script src="{{ asset('static/plugins/jquery-slimscroll/jquery.slimscroll.min.js') }}" type="text/javascript"></script>
        <script src="{{ asset('static/plugins/uniform/jquery.uniform.min.js') }}" type="text/javascript"></script>
        <script src="{{ asset('static/modules/admin/scripts/metronic.js') }}" type="text/javascript"></script>
        <script src="{{ asset('static/modules/admin/layout4/scripts/layout.js') }}" type="text/javascript"></script>
        {% block javascripts %}{% endblock %}
    </body>
</html>
