<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\RrlPush" table="rrl_push">
    <id name="rrlId" type="integer" column="rrl_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="bid" type="integer" column="bid" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="pushTime" type="integer" column="push_time" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">推送时间</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="pushBack" type="string" column="push_back" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">返回内容</option>
        <option name="default"/>
      </options>
    </field>
    <field name="pushDay" type="integer" column="push_day" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">日期ymd格式</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="code" type="integer" column="code" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
