<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\MallAddress" table="mall_address">
    <indexes>
      <index name="uid" columns="uid"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="uid" type="integer" column="uid" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">用户id</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="name" type="string" column="name" length="10" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">收货人姓名</option>
        <option name="default"/>
      </options>
    </field>
    <field name="mainPhone" type="string" column="main_phone" length="20" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">联系方式1</option>
        <option name="default"/>
      </options>
    </field>
    <field name="secondaryPhone" type="string" column="secondary_phone" length="20" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">联系方式2</option>
        <option name="default"/>
      </options>
    </field>
    <field name="address" type="string" column="address" length="100" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">地址</option>
        <option name="default"/>
      </options>
    </field>
    <field name="postCode" type="string" column="post_code" length="10" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">邮编</option>
        <option name="default"/>
      </options>
    </field>
    <field name="province" type="smallint" column="province" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">省代码</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="city" type="smallint" column="city" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">市代码</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="district" type="smallint" column="district" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">区代码</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="provinceText" type="string" column="province_text" length="100" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">省名</option>
        <option name="default"/>
      </options>
    </field>
    <field name="cityText" type="string" column="city_text" length="100" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">市名</option>
        <option name="default"/>
      </options>
    </field>
    <field name="districtText" type="string" column="district_text" length="100" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">区名</option>
        <option name="default"/>
      </options>
    </field>
    <field name="default" type="boolean" column="default" nullable="false">
      <options>
        <option name="comment">1:默认地址</option>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
