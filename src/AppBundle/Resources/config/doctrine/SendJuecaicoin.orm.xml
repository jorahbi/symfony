<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\SendJuecaicoin" table="send_juecaicoin">
    <unique-constraints>
      <unique-constraint name="order_on" columns="order_on"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="userid" type="integer" column="userId" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="sendtime" type="bigint" column="sendTime" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="investid" type="integer" column="investId" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">对应的投资记录ID</option>
      </options>
    </field>
    <field name="projectid" type="integer" column="projectId" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">对应投资项目ID</option>
      </options>
    </field>
    <field name="money" type="decimal" column="money" precision="10" scale="0" nullable="true">
      <options>
        <option name="comment">用户投资金额（维护性字段）</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="month" type="boolean" column="month" nullable="true">
      <options>
        <option name="comment">用户投资期限（月数）（维护性字段）</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="juecaibi" type="decimal" column="juecaibi" precision="10" scale="0" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="orderOn" type="string" column="order_on" length="31" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">赠送爵财币对应订单id</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
