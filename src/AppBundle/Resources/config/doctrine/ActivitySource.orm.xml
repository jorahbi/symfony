<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\ActivitySource" table="activity_source">
    <indexes>
      <index name="uid" columns="uid"/>
      <index name="channel" columns="channel"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="uid" type="integer" column="uid" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">用户ID</option>
      </options>
    </field>
    <field name="fromMsg" type="string" column="from_msg" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">活动来源</option>
      </options>
    </field>
    <field name="channel" type="string" column="channel" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">渠道</option>
      </options>
    </field>
    <field name="addtime" type="datetime" column="addtime" nullable="true"/>
  </entity>
</doctrine-mapping>
