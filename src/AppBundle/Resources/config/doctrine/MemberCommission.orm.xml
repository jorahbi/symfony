<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\MemberCommission" table="member_commission">
    <unique-constraints>
      <unique-constraint name="utype" columns="uid,ctype,friend_id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="uid" type="integer" column="uid" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">用户ID</option>
      </options>
    </field>
    <field name="friendId" type="integer" column="friend_id" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">好友ID</option>
      </options>
    </field>
    <field name="commission" type="decimal" column="commission" precision="15" scale="2" nullable="true">
      <options>
        <option name="comment">佣金</option>
      </options>
    </field>
    <field name="ctype" type="boolean" column="ctype" nullable="true">
      <options>
        <option name="comment">用户类型 commission_member_type.php</option>
        <option name="default">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
