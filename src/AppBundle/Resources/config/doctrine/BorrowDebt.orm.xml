<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\BorrowDebt" table="borrow_debt">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="investId" type="integer" column="invest_id" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">对应债权id</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="uid" type="integer" column="uid" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">转让人ID</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="pid" type="integer" column="pid" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">原标id</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="capital" type="decimal" column="capital" precision="10" scale="2" nullable="false">
      <options>
        <option name="comment">申请转让债权金额</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="interest" type="decimal" column="interest" precision="10" scale="2" nullable="false">
      <options>
        <option name="comment">转让的收益</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="duration" type="integer" column="duration" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">剩余期限（天）</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="durationMonth" type="decimal" column="duration_month" precision="10" scale="2" nullable="false">
      <options>
        <option name="comment">剩余期限（月）</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="discount" type="decimal" column="discount" precision="10" scale="2" nullable="false">
      <options>
        <option name="comment">折让金</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="defaultDiscount" type="decimal" column="default_discount" precision="10" scale="2" nullable="false">
      <options>
        <option name="comment">默认折让金</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="ownerRate" type="decimal" column="owner_rate" precision="6" scale="2" nullable="false">
      <options>
        <option name="comment">转让方转让后利率</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="buyerRate" type="decimal" column="buyer_rate" precision="6" scale="2" nullable="false">
      <options>
        <option name="comment">认购方利率</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="integral" type="integer" column="integral" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">转让积分</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="lsb" type="integer" column="lsb" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">转让利是币</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="addTime" type="integer" column="add_time" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">转让开始(申请)时间</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="endTime" type="integer" column="end_time" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">转让结束时间</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="fullTime" type="integer" column="full_time" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">转让完成时间</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="deadline" type="integer" column="deadline" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">最后一期还款时间</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="finialCapital" type="decimal" column="finial_capital" precision="10" scale="2" nullable="false">
      <options>
        <option name="comment">最终转让债权总额</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="finialInterest" type="decimal" column="finial_interest" precision="10" scale="2" nullable="false">
      <options>
        <option name="comment">最终转让收益</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="finialIntegral" type="integer" column="finial_integral" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">最终转让积分</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="finialLsb" type="integer" column="finial_lsb" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">最终转让利是币</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="finialDiscount" type="decimal" column="finial_discount" precision="10" scale="2" nullable="false">
      <options>
        <option name="comment">最终折让金</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="debtFee" type="decimal" column="debt_fee" precision="10" scale="2" nullable="false">
      <options>
        <option name="comment">转让手续费</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="status" type="boolean" column="status" nullable="false">
      <options>
        <option name="comment">1:待发布，2:可投资，3:转让完成，4:还款中，5:已取消</option>
        <option name="default">1</option>
      </options>
    </field>
    <field name="hits" type="integer" column="hits" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">关注人数</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="info" type="string" column="info" length="100" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">备注</option>
        <option name="default"/>
      </options>
    </field>
    <field name="remainPhase" type="integer" column="remain_phase" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="comment">剩余期限</option>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
