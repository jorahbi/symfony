<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\BorrowPackage" table="borrow_package">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="100" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">资产包编号</option>
        <option name="default"/>
      </options>
    </field>
    <field name="sourceName" type="string" column="source_name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">来源</option>
      </options>
    </field>
    <field name="packageType" type="boolean" column="package_type" nullable="true"/>
    <field name="totalMoney" type="decimal" column="total_money" precision="15" scale="2" nullable="false">
      <options>
        <option name="comment">资产包总金额</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="duration" type="string" column="duration" length="10" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">期限</option>
        <option name="default"/>
      </options>
    </field>
    <field name="rate" type="string" column="rate" length="20" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">利率</option>
        <option name="default"/>
      </options>
    </field>
    <field name="realRate" type="string" column="real_rate" length="20" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">实际利率</option>
        <option name="default"/>
      </options>
    </field>
    <field name="area" type="boolean" column="area" nullable="false">
      <options>
        <option name="comment">1:诚信众融专区，2:融资租赁专区，3:融资担保专区</option>
        <option name="default">1</option>
      </options>
    </field>
    <field name="contractType" type="boolean" column="contract_type" nullable="true"/>
    <field name="contractNumber" type="string" column="contract_number" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="borrowerCompany" type="string" column="borrower_company" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="deputy" type="string" column="deputy" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="businessLicence" type="string" column="business_licence" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="address" type="string" column="address" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="text" column="description" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="riskManagement" type="text" column="risk_management" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="pic" type="text" column="pic" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="serieName" type="string" column="serie_name" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">资产包系列名</option>
      </options>
    </field>
    <field name="orders" type="boolean" column="orders" nullable="true">
      <options>
        <option name="comment">排序</option>
      </options>
    </field>
    <field name="zdNumber" type="string" column="zd_number" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">中登网编号</option>
      </options>
    </field>
    <field name="projectCompany" type="string" column="project_company" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telephone" type="string" column="telephone" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idCard" type="string" column="id_card" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
