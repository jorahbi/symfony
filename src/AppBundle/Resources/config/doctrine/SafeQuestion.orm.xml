<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\SafeQuestion" table="safe_question">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="type" type="boolean" column="type" nullable="false">
      <options>
        <option name="comment">0:问题A,1:问题B</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="question" type="string" column="question" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">安全问题</option>
        <option name="default"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
