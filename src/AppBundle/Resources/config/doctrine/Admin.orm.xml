<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Admin" table="admin">
    <indexes>
      <index name="job_num,id" columns="job_num,id"/>
      <index name="IDX_IS_LF_JO" columns="is_service,lft,job_num"/>
      <index name="lft" columns="lft"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="adminName" type="string" column="admin_name" length="50" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">账号</option>
        <option name="default"/>
      </options>
    </field>
    <field name="adminPass" type="string" column="admin_pass" length="50" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">密码</option>
        <option name="default"/>
      </options>
    </field>
    <field name="adminLimit" type="text" column="admin_limit" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">权限</option>
      </options>
    </field>
    <field name="pid" type="integer" column="pid" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">父级ID</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="lft" type="integer" column="lft" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">左值</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="rgt" type="integer" column="rgt" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">右值</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="groupId" type="integer" column="group_id" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">部门ID</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="positionId" type="integer" column="position_id" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">职位ID</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="realName" type="string" column="real_name" length="20" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">真实姓名</option>
        <option name="default"/>
      </options>
    </field>
    <field name="jobNum" type="string" column="job_num" length="10" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">员工工号</option>
        <option name="default"/>
      </options>
    </field>
    <field name="phone" type="string" column="phone" length="18" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">坐席号码</option>
      </options>
    </field>
    <field name="qq" type="string" column="qq" length="20" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">企业QQ</option>
      </options>
    </field>
    <field name="sex" type="boolean" column="sex" nullable="false">
      <options>
        <option name="comment">性别0女士1先生</option>
      </options>
    </field>
    <field name="bbsUid" type="integer" column="bbs_uid" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">论坛管理员前台账号ID,对应members</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="valid" type="boolean" column="valid" nullable="false">
      <options>
        <option name="comment">是否有效：0禁用1有效</option>
        <option name="default">1</option>
      </options>
    </field>
    <field name="isService" type="boolean" column="is_service" nullable="false">
      <options>
        <option name="comment">是否客服</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="isInvestmentAdviser" type="boolean" column="is_investment_adviser" nullable="false">
      <options>
        <option name="comment">是否销售顾问</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="isBbsAdmin" type="boolean" column="is_bbs_admin" nullable="false">
      <options>
        <option name="comment">是否论坛管理员</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="isPartTime" type="boolean" column="is_part_time" nullable="false">
      <options>
        <option name="comment">是否兼职：1是，2否</option>
        <option name="default">2</option>
      </options>
    </field>
    <field name="isAutoAllot" type="boolean" column="is_auto_allot" nullable="false">
      <options>
        <option name="comment">是否自动分配客户：1分配，2不分配</option>
        <option name="default">2</option>
      </options>
    </field>
    <field name="skin" type="boolean" column="skin" nullable="false">
      <options>
        <option name="comment">后台皮肤</option>
        <option name="default">1</option>
      </options>
    </field>
    <field name="isServiceOnline" type="boolean" column="is_service_online" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="cno" type="integer" column="cno" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">客服工号</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
