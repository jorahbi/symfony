<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\BorrowSales" table="borrow_sales">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="aid" type="integer" column="aid" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">管理员ID，关联als_admin表</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="month" type="integer" column="month" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">月份，例如201503</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="moneyTotal" type="decimal" column="money_total" precision="15" scale="2" nullable="false">
      <options>
        <option name="comment">推荐人|客服本月业绩总额</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="moneyYear" type="decimal" column="money_year" precision="15" scale="2" nullable="false">
      <options>
        <option name="comment">年化业绩总额</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="brokerage" type="decimal" column="brokerage" precision="15" scale="2" nullable="false">
      <options>
        <option name="comment">本月佣金总额</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="brokerageExt" type="decimal" column="brokerage_ext" precision="15" scale="2" nullable="false">
      <options>
        <option name="comment">本月额外奖金</option>
        <option name="default">0.00</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
