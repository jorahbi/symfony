<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\BorrowInvest" table="borrow_invest">
    <indexes>
      <index name="investor_uid" columns="uid,status"/>
      <index name="borrow_id" columns="bid,uid,status"/>
      <index name="investor_uid2" columns="uid"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="bid" type="integer" column="bid" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">标的ID</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="debtId" type="integer" column="debt_id" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">债权转让子标id，0:非债权转让</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="uid" type="integer" column="uid" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">投资用户id</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="capital" type="decimal" column="capital" precision="15" scale="2" nullable="false">
      <options>
        <option name="comment">投标金额</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="capitalYear" type="decimal" column="capital_year" precision="15" scale="2" nullable="false">
      <options>
        <option name="comment">年化投资额</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="interest" type="decimal" column="interest" precision="15" scale="2" nullable="false">
      <options>
        <option name="comment">投资利息总额</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="fee" type="decimal" column="fee" precision="15" scale="2" nullable="false">
      <options>
        <option name="comment">投资者管理费(利息管理费)总数</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="receiveCapital" type="decimal" column="receive_capital" precision="15" scale="2" nullable="false">
      <options>
        <option name="comment">已返还投资额(本金)</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="receiveInterest" type="decimal" column="receive_interest" precision="15" scale="2" nullable="false">
      <options>
        <option name="comment">已返还利息(扣除管理费后)</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="paidFee" type="decimal" column="paid_fee" precision="15" scale="2" nullable="false">
      <options>
        <option name="comment">已收手续费(invest_fee)</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="status" type="boolean" column="status" nullable="false">
      <options>
        <option name="comment">1|预约中的投资,2|竞标中的投资,3|还款中的投资,4|还完的投资,5|取消的投资</option>
        <option name="default">1</option>
      </options>
    </field>
    <field name="dealStatus" type="boolean" column="deal_status" nullable="false">
      <options>
        <option name="comment">回调处理状态：1|处理中，2|已处理</option>
        <option name="default">2</option>
      </options>
    </field>
    <field name="discount" type="decimal" column="discount" precision="15" scale="2" nullable="false">
      <options>
        <option name="comment">投资折让金（用于债权转让）</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="orderNo" type="string" column="order_no" length="32" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">订单号</option>
        <option name="default"/>
      </options>
    </field>
    <field name="addTime" type="integer" column="add_time" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">投资时间</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="addMs" type="smallint" column="add_ms" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">毫秒</option>
        <option name="default">0000</option>
      </options>
    </field>
    <field name="deadline" type="integer" column="deadline" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">最后一期还款时间</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="isAuto" type="boolean" column="is_auto" nullable="false">
      <options>
        <option name="comment">是否自动投资1:是2:否</option>
        <option name="default">2</option>
      </options>
    </field>
    <field name="client" type="boolean" column="client" nullable="false">
      <options>
        <option name="comment">1:web,2:android,3:ios,4:wap,5:weixin,6移动</option>
        <option name="default">1</option>
      </options>
    </field>
    <field name="serviceId" type="integer" column="service_id" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="sale" type="decimal" column="sale" precision="15" scale="2" nullable="false">
      <options>
        <option name="comment">投标抵扣金（红包抵用）</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="jobNum" type="string" column="job_num" length="10" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">佣金所有者工号,关联als_admin表</option>
        <option name="default"/>
      </options>
    </field>
    <field name="brokerage" type="decimal" column="brokerage" precision="15" scale="2" nullable="false">
      <options>
        <option name="comment">佣金</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="brokerageExt" type="decimal" column="brokerage_ext" precision="15" scale="2" nullable="false">
      <options>
        <option name="comment">奖金</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="brokerageType" type="boolean" column="brokerage_type" nullable="false">
      <options>
        <option name="comment">佣金类型：1早期佣金数据,2推广用户,3自拓用户</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="isFirst" type="boolean" column="is_first" nullable="false">
      <options>
        <option name="comment">是否首投：1|是，0|否</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="rate" type="decimal" column="rate" precision="5" scale="2" nullable="true">
      <options>
        <option name="comment">体验表利率，正常标为0</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="isBeyond" type="integer" column="is_beyond" nullable="true">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">是否超投自动投资1:是</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="addIp" type="string" column="add_ip" length="15" nullable="true">
      <options>
        <option name="fixed">1</option>
        <option name="default"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
