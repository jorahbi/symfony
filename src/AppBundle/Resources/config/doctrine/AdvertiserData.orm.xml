<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\AdvertiserData" table="advertiser_data">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="recordDate" type="string" column="record_date" length="20" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">记录日期</option>
      </options>
    </field>
    <field name="regNum" type="integer" column="reg_num" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">注册人数</option>
      </options>
    </field>
    <field name="realNum" type="integer" column="real_num" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">实名人数</option>
      </options>
    </field>
    <field name="cardNum" type="integer" column="card_num" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">绑卡人数</option>
      </options>
    </field>
    <field name="investNum" type="integer" column="invest_num" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">投资人数</option>
      </options>
    </field>
    <field name="chargeNum" type="integer" column="charge_num" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">充值人数</option>
      </options>
    </field>
    <field name="chargeMoney" type="decimal" column="charge_money" precision="15" scale="2" nullable="true">
      <options>
        <option name="comment">充值总金额</option>
      </options>
    </field>
    <field name="investMoney" type="decimal" column="invest_money" precision="15" scale="2" nullable="true">
      <options>
        <option name="comment">投资总金额</option>
      </options>
    </field>
    <field name="drawMoney" type="decimal" column="draw_money" precision="15" scale="2" nullable="true">
      <options>
        <option name="comment">提现总金额</option>
      </options>
    </field>
    <field name="firstUser" type="integer" column="first_user" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">首投人數</option>
      </options>
    </field>
    <field name="againUser" type="integer" column="again_user" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">复投人数</option>
      </options>
    </field>
    <field name="aveInvest" type="decimal" column="ave_invest" precision="15" scale="2" nullable="true">
      <options>
        <option name="comment">客单价</option>
      </options>
    </field>
    <field name="firstInvest" type="decimal" column="first_invest" precision="15" scale="2" nullable="true">
      <options>
        <option name="comment">首投金額</option>
      </options>
    </field>
    <field name="afterInvest" type="decimal" column="after_invest" precision="15" scale="2" nullable="true">
      <options>
        <option name="comment">羊毛金額（注册三天后）</option>
      </options>
    </field>
    <field name="againInvest" type="decimal" column="again_invest" precision="15" scale="2" nullable="true">
      <options>
        <option name="comment">复投金额</option>
      </options>
    </field>
    <field name="code" type="integer" column="code" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">标识。0为日记录，1为周记录，2为月记录</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="channel" type="string" column="channel" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">渠道</option>
        <option name="default"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
