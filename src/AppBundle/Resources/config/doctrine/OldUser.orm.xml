<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\OldUser" table="old_user">
    <indexes>
      <index name="attestation_id" columns="attestation_id"/>
    </indexes>
    <id name="userId" type="integer" column="user_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="typeId" type="integer" column="type_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="attestationId" type="integer" column="attestation_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="userAccount" type="string" column="user_account" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userPassword" type="string" column="user_password" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userPaypassword" type="string" column="user_paypassword" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userRealname" type="string" column="user_realname" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userTel" type="string" column="user_tel" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userSex" type="string" column="user_sex" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userQq" type="string" column="user_qq" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userBirthday" type="datetime" column="user_birthday" nullable="true"/>
    <field name="userProvince" type="string" column="user_province" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userCity" type="string" column="user_city" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userArea" type="string" column="user_area" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="userType" type="string" column="user_type" length="100" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">所属客服</option>
      </options>
    </field>
    <field name="applyDsc" type="string" column="apply_dsc" length="100" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">申请说明</option>
      </options>
    </field>
    <field name="applyTime" type="datetime" column="apply_time" nullable="true">
      <options>
        <option name="comment">申请时间</option>
      </options>
    </field>
    <field name="userStatus" type="integer" column="user_status" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">是否客服</option>
        <option name="default">-1</option>
      </options>
    </field>
    <field name="filesUrl" type="string" column="files_url" length="220" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">上传图片路径</option>
      </options>
    </field>
    <field name="nation" type="string" column="nation" length="10" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">民族</option>
      </options>
    </field>
    <field name="infoCompleted" type="integer" column="info_completed" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">资料完整度</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="isLock" type="integer" column="is_lock" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">0,不锁定  1，锁定</option>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
