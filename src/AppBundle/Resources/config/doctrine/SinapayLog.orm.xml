<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\SinapayLog" table="sinapay_log">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="uid" type="integer" column="uid" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">用户id</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="apiName" type="string" column="api_name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">接口名称</option>
        <option name="default"/>
      </options>
    </field>
    <field name="queryJson" type="text" column="query_json" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">请求参数</option>
      </options>
    </field>
    <field name="result" type="string" column="result" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">返回结果</option>
        <option name="default"/>
      </options>
    </field>
    <field name="addTime" type="integer" column="add_time" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">日志产生时间</option>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
