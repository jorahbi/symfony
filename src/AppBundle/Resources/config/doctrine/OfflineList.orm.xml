<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\OfflineList" table="offline_list">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="realName" type="string" column="real_name" length="150" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">姓名</option>
      </options>
    </field>
    <field name="userPhone" type="string" column="user_phone" length="33" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">电话</option>
      </options>
    </field>
    <field name="serviceId" type="integer" column="service_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="comment">客服id</option>
      </options>
    </field>
    <field name="status" type="boolean" column="status" nullable="false">
      <options>
        <option name="comment">1新客户2意向客户3潜在客户4成交客户5无效客户6无效号码</option>
        <option name="default">1</option>
      </options>
    </field>
    <field name="isCall" type="integer" column="is_call" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="comment">1已电询2未电询</option>
        <option name="default">2</option>
      </options>
    </field>
    <field name="createTime" type="integer" column="create_time" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="comment">导入时间</option>
      </options>
    </field>
    <field name="uid" type="integer" column="uid" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="comment">对应members表id</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="isReg" type="boolean" column="is_reg" nullable="false">
      <options>
        <option name="comment">0未注册1注册</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="desc" type="text" column="desc" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">注释描述</option>
      </options>
    </field>
    <field name="nextCallTime" type="integer" column="next_call_time" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">下次预约时间</option>
      </options>
    </field>
    <field name="investmentHistory" type="text" column="investment_history" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">投资历史</option>
      </options>
    </field>
    <field name="assetsInfo" type="string" column="assets_info" length="500" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">资产情况</option>
      </options>
    </field>
    <field name="age" type="boolean" column="age" nullable="true">
      <options>
        <option name="comment">年龄</option>
      </options>
    </field>
    <field name="email" type="string" column="email" length="50" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">Email</option>
      </options>
    </field>
    <field name="sex" type="boolean" column="sex" nullable="true">
      <options>
        <option name="comment">性别1男2女</option>
      </options>
    </field>
    <field name="areaL1" type="integer" column="area_l1" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">区域分类1级</option>
      </options>
    </field>
    <field name="areaL2" type="integer" column="area_l2" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">区域分类2级</option>
      </options>
    </field>
    <field name="areaL3" type="integer" column="area_l3" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">区域分类3级</option>
      </options>
    </field>
    <field name="areaL4" type="integer" column="area_l4" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">区域分类4级</option>
      </options>
    </field>
    <field name="industryL1" type="string" column="industry_l1" length="11" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">行业分类1级</option>
      </options>
    </field>
    <field name="industryL2" type="string" column="industry_l2" length="100" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">行业分类2级</option>
      </options>
    </field>
    <field name="industryL3" type="string" column="industry_l3" length="100" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">行业分类3级</option>
      </options>
    </field>
    <field name="industryL4" type="string" column="industry_l4" length="100" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">行业分类4级</option>
      </options>
    </field>
    <field name="areaNameL1" type="string" column="area_name_l1" length="100" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">区域分类1级中文名称</option>
      </options>
    </field>
    <field name="areaNameL2" type="string" column="area_name_l2" length="100" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">区域分类2级中文名称</option>
      </options>
    </field>
    <field name="areaNameL3" type="string" column="area_name_l3" length="100" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">区域分类3级中文名称</option>
      </options>
    </field>
    <field name="areaNameL4" type="string" column="area_name_l4" length="100" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">区域分类4级中文名称</option>
      </options>
    </field>
    <field name="industryNameL1" type="string" column="industry_name_l1" length="100" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">行业分类1级中文名称</option>
      </options>
    </field>
    <field name="industryNameL2" type="string" column="industry_name_l2" length="100" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">行业分类2级中文名称</option>
      </options>
    </field>
    <field name="industryNameL3" type="string" column="industry_name_l3" length="100" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">行业分类3级中文名称</option>
      </options>
    </field>
    <field name="industryNameL4" type="string" column="industry_name_l4" length="100" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">行业分类4级中文名称</option>
      </options>
    </field>
    <field name="lastTime" type="integer" column="last_time" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">最后通话时间</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="toolsSource" type="boolean" column="tools_source" nullable="true">
      <options>
        <option name="comment">访问渠道:1固话2QQ3微信4邮件5短信6手机7其他</option>
        <option name="default">1</option>
      </options>
    </field>
    <field name="listenStatus" type="boolean" column="listen_status" nullable="true">
      <options>
        <option name="comment">接听状态:1接听2无人接听3关机4停机5空号</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="spread" type="boolean" column="spread" nullable="true">
      <options>
        <option name="comment">推广数据:1正常2非本人注册3融资借贷4恶意注册</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="channels" type="boolean" column="channels" nullable="true">
      <options>
        <option name="comment">客户希望联系渠道:1短信2微信3qq4邮件5电话6手机</option>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
