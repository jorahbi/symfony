<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\TempLuck" table="temp_luck">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="bid" type="integer" column="bid" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="addTime" type="integer" column="add_time" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="prizeId" type="boolean" column="prize_id" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="userPhone" type="string" column="user_phone" length="11" nullable="true">
      <options>
        <option name="fixed">1</option>
        <option name="default"/>
      </options>
    </field>
    <field name="prizePrice" type="boolean" column="prize_price" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
