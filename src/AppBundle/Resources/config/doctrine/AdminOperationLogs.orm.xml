<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\AdminOperationLogs" table="admin_operation_logs">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="url" type="string" column="url" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">URL</option>
      </options>
    </field>
    <field name="operator" type="integer" column="operator" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="comment">操作人</option>
      </options>
    </field>
    <field name="description" type="string" column="description" length="60" nullable="false">
      <options>
        <option name="fixed">1</option>
        <option name="comment">描述</option>
      </options>
    </field>
    <field name="operateTime" type="integer" column="operate_time" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="comment">操作时间</option>
      </options>
    </field>
    <field name="operateType" type="boolean" column="operate_type" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
