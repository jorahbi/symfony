<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Team" table="team">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="teamName" type="string" column="team_name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">战队名</option>
      </options>
    </field>
    <field name="totalInvest" type="decimal" column="total_invest" precision="50" scale="2" nullable="false">
      <options>
        <option name="comment">总投资额</option>
        <option name="default">0.00</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
