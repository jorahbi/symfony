<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\MemberReceive" table="member_receive">
    <indexes>
      <index name="uid" columns="uid"/>
      <index name="tid" columns="uid"/>
    </indexes>
    <id name="receiveId" type="integer" column="receive_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="uid" type="integer" column="uid" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">uid</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="affectMoney" type="decimal" column="affect_money" precision="15" scale="2" nullable="true">
      <options>
        <option name="comment">影响金额</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="receiveBalance" type="decimal" column="receive_balance" precision="15" scale="2" nullable="true">
      <options>
        <option name="comment">回款池余额</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="affectType" type="boolean" column="affect_type" nullable="true">
      <options>
        <option name="comment">0：初始明细 &#13;
1：回款 &#13;
2：提现 &#13;
3：投资 </option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="addTime" type="integer" column="add_time" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">添加时间</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="tid" type="integer" column="tid" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">关联表的主键</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="bonusMoney" type="decimal" column="bonus_money" precision="15" scale="2" nullable="true">
      <options>
        <option name="comment">续投奖金</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="bonusStatus" type="boolean" column="bonus_status" nullable="true">
      <options>
        <option name="comment">0：没有状态，&#13;
1：等待回调，&#13;
2：发放失败，&#13;
3：发放成功</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="orderNo" type="string" column="order_no" length="30" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">代收订单号</option>
        <option name="default"/>
      </options>
    </field>
    <field name="payOrderNo" type="string" column="pay_order_no" length="30" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">代付订单号</option>
        <option name="default"/>
      </options>
    </field>
    <field name="bid" type="integer" column="bid" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">标ID</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="redNum" type="integer" column="red_num" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">红包张数</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="redMoney" type="integer" column="red_money" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">红包面额</option>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
