<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\RegAssignLog" table="reg_assign_log">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="totalRegNumber" type="integer" column="total_reg_number" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="comment">注册总人数</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="avgRegNumber" type="integer" column="avg_reg_number" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="comment">分配平均人数</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="serviceNumber" type="integer" column="service_number" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="comment">客服人数</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="text" type="text" column="text" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">文本</option>
      </options>
    </field>
    <field name="membersSql" type="text" column="members_sql" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">修改members sql</option>
      </options>
    </field>
    <field name="memberinfoSql" type="text" column="memberinfo_sql" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">修改memberinfo sql</option>
      </options>
    </field>
    <field name="addTime" type="integer" column="add_time" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">记录日志时间</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="type" type="integer" column="type" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">4线上5线下</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="status" type="integer" column="status" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">1:注册分配2：回收 分配</option>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
