<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\UserJuecaicoin" table="user_juecaicoin">
    <unique-constraints>
      <unique-constraint name="user_createtime" columns="userId,createtime"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="userid" type="integer" column="userId" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="allmoney" type="decimal" column="allMoney" precision="10" scale="0" nullable="true">
      <options>
        <option name="comment">一共获取爵财币数目</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="currentmoney" type="decimal" column="currentMoney" precision="10" scale="0" nullable="true">
      <options>
        <option name="comment">当前爵财币数目</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="addfrequency" type="integer" column="addfrequency" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">该用户新增爵财币数目的次数</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="consumemoney" type="integer" column="consumeMoney" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">该用户消耗爵财币数目的次数</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="status" type="boolean" column="status" nullable="true">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="createtime" type="integer" column="createtime" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
