<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Queue" table="queue">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="status" type="boolean" column="status" nullable="false">
      <options>
        <option name="comment">状态：0未处理，1已处理</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="func" type="string" column="func" length="100" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">队列处理方法</option>
        <option name="default"/>
      </options>
    </field>
    <field name="param" type="string" column="param" length="100" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">参数，格式自定，在设置的方法中做相应处理即可</option>
        <option name="default"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
