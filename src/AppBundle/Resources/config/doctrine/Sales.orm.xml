<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Sales" table="sales">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="salesId" type="integer" column="sales_id" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">销售员ID，关联als_admin_sales表</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="month" type="integer" column="month" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">月份，例如201502</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="capital" type="decimal" column="capital" precision="15" scale="2" nullable="false">
      <options>
        <option name="comment">推荐人本月投资总额</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="capitalYear" type="decimal" column="capital_year" precision="15" scale="2" nullable="false">
      <options>
        <option name="comment">年化业绩</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="money" type="decimal" column="money" precision="15" scale="2" nullable="false">
      <options>
        <option name="comment">本月佣金0.2%年化奖励</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="moneyExtra" type="decimal" column="money_extra" precision="15" scale="2" nullable="false">
      <options>
        <option name="comment">首投1%年化奖励</option>
        <option name="default">0.00</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
