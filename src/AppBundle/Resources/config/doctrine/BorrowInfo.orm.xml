<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\BorrowInfo" table="borrow_info">
    <indexes>
      <index name="type_list" columns="borrow_type,is_show"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="package" type="string" column="package" length="20" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">资产包编号</option>
        <option name="default"/>
      </options>
    </field>
    <field name="classify" type="string" column="classify" length="50" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">P2B项目主营类型</option>
        <option name="default"/>
      </options>
    </field>
    <field name="borrowName" type="string" column="borrow_name" length="50" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">标名</option>
        <option name="default"/>
      </options>
    </field>
    <field name="borrowOrder" type="string" column="borrow_order" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">借款编号</option>
      </options>
    </field>
    <field name="borrowUid" type="integer" column="borrow_uid" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">借款人id</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="borrowType" type="boolean" column="borrow_type" nullable="false">
      <options>
        <option name="comment">借款类型:(1 : p2p; 3 : P2B)</option>
        <option name="default">3</option>
      </options>
    </field>
    <field name="duration" type="integer" column="duration" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">借款周期</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="durationUnit" type="string" column="duration_unit" length="10" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">期限单位</option>
        <option name="default">个月</option>
      </options>
    </field>
    <field name="durationMonth" type="decimal" column="duration_month" precision="10" scale="2" nullable="false">
      <options>
        <option name="comment">期限月数</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="durationOriginal" type="integer" column="duration_original" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">原始期限（针对于贷款方）</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="money" type="decimal" column="money" precision="15" scale="2" nullable="false">
      <options>
        <option name="comment">借款总金额</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="interest" type="decimal" column="interest" precision="15" scale="2" nullable="false">
      <options>
        <option name="comment">借款总利息</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="finialMoney" type="decimal" column="finial_money" precision="15" scale="2" nullable="false">
      <options>
        <option name="comment">最终投标金额</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="finialInterest" type="decimal" column="finial_interest" precision="15" scale="2" nullable="false">
      <options>
        <option name="comment">最终利息</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="finialBaseInterest" type="decimal" column="finial_base_interest" precision="15" scale="2" nullable="false">
      <options>
        <option name="comment">最终应收总利息（针对贷款方）</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="receiveMoney" type="decimal" column="receive_money" precision="15" scale="2" nullable="false">
      <options>
        <option name="comment">实还本金</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="receiveInterest" type="decimal" column="receive_interest" precision="15" scale="2" nullable="false">
      <options>
        <option name="comment">实还利息</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="interestRate" type="decimal" column="interest_rate" precision="5" scale="2" nullable="false">
      <options>
        <option name="comment">年化率</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="baseRate" type="decimal" column="base_rate" precision="5" scale="2" nullable="false">
      <options>
        <option name="comment">基础利率（针对于贷款方）</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="alsRate" type="decimal" column="als_rate" precision="5" scale="2" nullable="false">
      <options>
        <option name="comment">平台补贴利率(仅做显示用)</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="repaymentType" type="boolean" column="repayment_type" nullable="false">
      <options>
        <option name="comment">1每月付息，2等额本息</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="repaymentDay" type="boolean" column="repayment_day" nullable="false">
      <options>
        <option name="comment">每月固定还款日期</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="borrowStatus" type="boolean" column="borrow_status" nullable="false">
      <options>
        <option name="comment">标状态:0:待审核 1:审核未通过 2:审核通过 3:投标完成还款中 4:还款全部完成 5:流标</option>
        <option name="default">1</option>
      </options>
    </field>
    <field name="cover" type="string" column="cover" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">标的封面图片</option>
        <option name="default"/>
      </options>
    </field>
    <field name="startPrice" type="decimal" column="start_price" precision="15" scale="2" nullable="false">
      <options>
        <option name="comment">起投金额</option>
        <option name="default">100.00</option>
      </options>
    </field>
    <field name="upperPrice" type="decimal" column="upper_price" precision="15" scale="2" nullable="false">
      <options>
        <option name="comment">标金额上限</option>
      </options>
    </field>
    <field name="perPrice" type="decimal" column="per_price" precision="15" scale="2" nullable="false">
      <options>
        <option name="comment">每份投资额</option>
        <option name="default">100.00</option>
      </options>
    </field>
    <field name="startTime" type="integer" column="start_time" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">资产包开始时间</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="hasMoney" type="decimal" column="has_money" precision="15" scale="2" nullable="true">
      <options>
        <option name="comment">已投金额</option>
        <option name="default">0.00</option>
      </options>
    </field>
    <field name="openTime" type="integer" column="open_time" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">项目开始投资时间</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="endTime" type="integer" column="end_time" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">投资截止时间</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="fullTime" type="integer" column="full_time" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">满标时间</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="deadline" type="integer" column="deadline" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">还款全部完成时间</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="isShow" type="boolean" column="is_show" nullable="false">
      <options>
        <option name="comment">是否显示标：0隐藏，1显示</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="isAuto" type="boolean" column="is_auto" nullable="false">
      <options>
        <option name="comment">是否可自动投资：0否，1是</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="isOverdue" type="boolean" column="is_overdue" nullable="false">
      <options>
        <option name="comment">是否逾期0:否，1:是，2:逾期结束</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="isLoan" type="boolean" column="is_loan" nullable="false">
      <options>
        <option name="comment">0|未申请放款，1|已申请放款</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="couponsType" type="boolean" column="coupons_type" nullable="false">
      <options>
        <option name="comment">可使用的红包类型：0|不能使用</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="hits" type="integer" column="hits" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">点击量</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="risk" type="text" column="risk" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">风险控制</option>
      </options>
    </field>
    <field name="danbaoName" type="string" column="danbao_name" length="50" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">担保人名称</option>
        <option name="default"/>
      </options>
    </field>
    <field name="zhaiwuName" type="string" column="zhaiwu_name" length="50" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">债务人名称</option>
        <option name="default"/>
      </options>
    </field>
    <field name="zhaiwuIdcard" type="string" column="zhaiwu_idcard" length="50" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">债务人身份证</option>
        <option name="default"/>
      </options>
    </field>
    <field name="zhaiwuCompany" type="string" column="zhaiwu_company" length="50" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">债务人企业编号</option>
        <option name="default"/>
      </options>
    </field>
    <field name="pactType" type="boolean" column="pact_type" nullable="false">
      <options>
        <option name="comment">合同类型：1融资租赁，2融资担保</option>
        <option name="default">1</option>
      </options>
    </field>
    <field name="pactZhang" type="boolean" column="pact_zhang" nullable="false">
      <options>
        <option name="comment">合同章：1上海锦轩融资租赁有限公司</option>
        <option name="default">1</option>
      </options>
    </field>
    <field name="rongziyongtu" type="text" column="rongziyongtu" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">融资用途</option>
      </options>
    </field>
    <field name="saleAuto" type="boolean" column="sale_auto" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="addTime" type="integer" column="add_time" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">添加时间</option>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
