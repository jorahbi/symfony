<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\AdminServiceLog" table="admin_service_log">
    <indexes>
      <index name="uid" columns="uid"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="uid" type="integer" column="uid" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">用户ID</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="type" type="boolean" column="type" nullable="false">
      <options>
        <option name="comment">类型：1线上注册，2线下导入 3名单导入 4注册线上自动分配 5注册线下自动分配</option>
        <option name="default">1</option>
      </options>
    </field>
    <field name="oldServiceId" type="integer" column="old_service_id" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">原客服ID</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="oldName" type="string" column="old_name" length="15" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">原客服姓名</option>
        <option name="default"/>
      </options>
    </field>
    <field name="newServiceId" type="integer" column="new_service_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="comment">新客服ID</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="newName" type="string" column="new_name" length="15" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">新客服姓名</option>
        <option name="default"/>
      </options>
    </field>
    <field name="addTime" type="integer" column="add_time" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">添加时间</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="addDay" type="integer" column="add_day" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">添加时间(天)</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="allocationType" type="integer" column="allocation_type" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">自动分配类型1：注册分配2回收分配</option>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
