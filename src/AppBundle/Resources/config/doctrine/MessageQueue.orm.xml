<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\MessageQueue" table="message_queue">
    <indexes>
      <index name="status" columns="status"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="action" type="string" column="action" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">方法名</option>
      </options>
    </field>
    <field name="param" type="string" column="param" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">参数</option>
      </options>
    </field>
    <field name="addTime" type="datetime" column="add_time" nullable="true"/>
    <field name="status" type="boolean" column="status" nullable="true">
      <options>
        <option name="comment">执行状态，1执行</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="info" type="string" column="info" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
