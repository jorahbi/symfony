<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\WithdrawTicket" table="withdraw_ticket">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="uid" type="integer" column="uid" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">用户ID</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="tid" type="integer" column="tid" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">提现记录ID</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="endTime" type="integer" column="end_time" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">截止时间</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="useTime" type="integer" column="use_time" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">使用时间</option>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
