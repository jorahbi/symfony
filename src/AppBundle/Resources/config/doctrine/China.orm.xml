<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\China" table="china">
    <indexes>
      <index name="FK_CHINA_REFERENCE_CHINA" columns="pid"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="40" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">地区名称</option>
      </options>
    </field>
    <field name="pid" type="integer" column="pid" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">上级地区ID</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
