<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\OperateDataCount" table="operate_data_count">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="recordDate" type="string" column="record_date" length="20" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">记录日期</option>
      </options>
    </field>
    <field name="regNum" type="integer" column="reg_num" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">注册人数</option>
      </options>
    </field>
    <field name="realNum" type="integer" column="real_num" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">实名人数</option>
      </options>
    </field>
    <field name="cardNum" type="integer" column="card_num" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">绑卡人数</option>
      </options>
    </field>
    <field name="investNum" type="integer" column="invest_num" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">投资人数</option>
      </options>
    </field>
    <field name="chargeNum" type="integer" column="charge_num" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">充值人数</option>
      </options>
    </field>
    <field name="chargeMoney" type="decimal" column="charge_money" precision="15" scale="2" nullable="true">
      <options>
        <option name="comment">充值总金额</option>
      </options>
    </field>
    <field name="investMoney" type="decimal" column="invest_money" precision="15" scale="2" nullable="true">
      <options>
        <option name="comment">投资总金额</option>
      </options>
    </field>
    <field name="drawMoney" type="decimal" column="draw_money" precision="15" scale="2" nullable="true">
      <options>
        <option name="comment">提现总金额</option>
      </options>
    </field>
    <field name="againUser" type="integer" column="again_user" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">复投人数</option>
      </options>
    </field>
    <field name="aveInvest" type="decimal" column="ave_invest" precision="15" scale="2" nullable="true">
      <options>
        <option name="comment">客单价</option>
      </options>
    </field>
    <field name="code" type="integer" column="code" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">标识。0为周记录，1为月记录</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="firstInvestNum" type="integer" column="first_invest_num" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">首投人数</option>
      </options>
    </field>
    <field name="firstInvestMoney" type="decimal" column="first_invest_money" precision="15" scale="2" nullable="true">
      <options>
        <option name="comment">首投金额</option>
      </options>
    </field>
    <field name="againMoney" type="decimal" column="again_money" precision="15" scale="2" nullable="true">
      <options>
        <option name="comment">复投金额</option>
      </options>
    </field>
    <field name="repayMoney" type="decimal" column="repay_money" precision="15" scale="2" nullable="true">
      <options>
        <option name="comment">回款金额</option>
      </options>
    </field>
    <field name="comDrawMoney" type="decimal" column="com_draw_money" precision="15" scale="2" nullable="true">
      <options>
        <option name="comment">企业提现总额</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
