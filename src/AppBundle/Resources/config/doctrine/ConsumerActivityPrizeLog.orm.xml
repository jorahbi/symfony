<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\ConsumerActivityPrizeLog" table="consumer_activity_prize_log">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="uid" type="integer" column="uid" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="comment">用户id</option>
      </options>
    </field>
    <field name="prizeId" type="integer" column="prize_id" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="comment">奖品id</option>
      </options>
    </field>
    <field name="addtime" type="integer" column="addtime" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">奖品添加时间</option>
      </options>
    </field>
    <field name="status" type="integer" column="status" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="comment">状态：0、未发放；1、已发放</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="grantTime" type="integer" column="grant_time" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">奖励发放时间</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
