<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\MemberBank" table="member_bank">
    <indexes>
      <index name="uid" columns="uid"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="uid" type="integer" column="uid" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">用户id</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="cardId" type="string" column="card_id" length="50" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">钱包系统卡ID</option>
        <option name="default"/>
      </options>
    </field>
    <field name="bankCode" type="string" column="bank_code" length="50" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">银行编码</option>
        <option name="default"/>
      </options>
    </field>
    <field name="bankName" type="string" column="bank_name" length="50" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">银行名称</option>
        <option name="default"/>
      </options>
    </field>
    <field name="bankAccountNo" type="string" column="bank_account_no" length="50" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">银行卡号</option>
        <option name="default"/>
      </options>
    </field>
    <field name="province" type="string" column="province" length="20" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">开户省</option>
        <option name="default"/>
      </options>
    </field>
    <field name="city" type="string" column="city" length="20" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">开户市</option>
        <option name="default"/>
      </options>
    </field>
    <field name="bankBranch" type="string" column="bank_branch" length="100" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">支行名称</option>
        <option name="default"/>
      </options>
    </field>
    <field name="addTime" type="integer" column="add_time" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">添加时间</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="addIp" type="string" column="add_ip" length="16" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">添加ip</option>
        <option name="default"/>
      </options>
    </field>
    <field name="status" type="boolean" column="status" nullable="false">
      <options>
        <option name="comment">状态：1|有效，2|无效</option>
        <option name="default">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
