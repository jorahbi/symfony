<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\MemberLogin" table="member_login">
    <unique-constraints>
      <unique-constraint name="uid" columns="uid"/>
    </unique-constraints>
    <id name="uid" type="integer" column="uid">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="ip" type="string" column="ip" length="20" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">IP地址</option>
        <option name="default"/>
      </options>
    </field>
    <field name="address" type="string" column="address" length="20" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">登录地址</option>
        <option name="default"/>
      </options>
    </field>
    <field name="browser" type="string" column="browser" length="20" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">浏览器类型</option>
        <option name="default"/>
      </options>
    </field>
    <field name="client" type="string" column="client" length="20" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">终端</option>
        <option name="default"/>
      </options>
    </field>
    <field name="loginType" type="string" column="login_type" length="20" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">账号类型</option>
        <option name="default"/>
      </options>
    </field>
    <field name="time" type="integer" column="time" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">登录时间</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="logs" type="text" column="logs" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">最近5次登录信息</option>
      </options>
    </field>
    <field name="lock" type="boolean" column="lock" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
