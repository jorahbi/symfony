<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\MemberCoupons" table="member_coupons">
    <indexes>
      <index name="type" columns="type"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="uid" type="integer" column="uid" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">用户ID</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="getId" type="integer" column="get_id" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">获得投资券的记录id,例如:投资记录id,充值记录id....</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="getBid" type="integer" column="get_bid" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">获得投资券的标id</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="getType" type="boolean" column="get_type" nullable="false">
      <options>
        <option name="comment">来源类型，1注册,2充值,3投资...见coupons_get_type.php</option>
        <option name="default">1</option>
      </options>
    </field>
    <field name="getInfo" type="string" column="get_info" length="200" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">获得途径备注</option>
        <option name="default"/>
      </options>
    </field>
    <field name="useBid" type="integer" column="use_bid" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">使用投资券的标id</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="useIid" type="integer" column="use_iid" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">使用投资券的投资记录id</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="money" type="integer" column="money" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">面额</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="baseMoney" type="integer" column="base_money" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">优惠券使用条件，投资金额要求</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="type" type="boolean" column="type" nullable="false">
      <options>
        <option name="comment">投资券类型，例如1代表50/1000...见coupons_type.php</option>
        <option name="default">1</option>
      </options>
    </field>
    <field name="addTime" type="integer" column="add_time" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">获得时间</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="startTime" type="integer" column="start_time" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">生效时间</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="endTime" type="integer" column="end_time" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">截止时间</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="useTime" type="integer" column="use_time" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="comment">使用时间</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="isManual" type="integer" column="is_manual" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">是否手动添加的红包 1:是</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="month" type="string" column="month" length="100" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="comment">标的期限 0:不限期限</option>
      </options>
    </field>
    <field name="shareId" type="integer" column="share_id" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">所获取红包的分享来源id（针对分享抢红包类型活动）</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
