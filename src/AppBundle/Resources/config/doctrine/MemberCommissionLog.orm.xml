<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\MemberCommissionLog" table="member_commission_log">
    <indexes>
      <index name="bid" columns="bid"/>
      <index name="repay_id" columns="invest_id"/>
      <index name="cid" columns="cid"/>
      <index name="status" columns="status"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="cid_invest" columns="cid,invest_id"/>
      <unique-constraint name="order_no" columns="order_no"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="cid" type="integer" column="cid" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">佣金用户ID</option>
      </options>
    </field>
    <field name="bid" type="integer" column="bid" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">标ID</option>
      </options>
    </field>
    <field name="investId" type="integer" column="invest_id" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">投资记录ID</option>
      </options>
    </field>
    <field name="interest" type="decimal" column="interest" precision="15" scale="2" nullable="true">
      <options>
        <option name="comment">利息</option>
      </options>
    </field>
    <field name="commission" type="decimal" column="commission" precision="15" scale="2" nullable="true">
      <options>
        <option name="comment">佣金</option>
      </options>
    </field>
    <field name="ratio" type="decimal" column="ratio" precision="3" scale="2" nullable="true">
      <options>
        <option name="comment">比例</option>
      </options>
    </field>
    <field name="status" type="boolean" column="status" nullable="true">
      <options>
        <option name="comment">处理状态 0未处理 1已处理</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="orderNo" type="string" column="order_no" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">订单号</option>
      </options>
    </field>
    <field name="addTime" type="datetime" column="add_time" nullable="true">
      <options>
        <option name="comment">获取时间</option>
      </options>
    </field>
    <field name="lasttime" type="datetime" column="lasttime" nullable="true">
      <options>
        <option name="comment">处理时间</option>
      </options>
    </field>
    <field name="remarks" type="string" column="remarks" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">备注信息</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
