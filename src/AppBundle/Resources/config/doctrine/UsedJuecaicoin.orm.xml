<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\UsedJuecaicoin" table="used_juecaicoin">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="uid" type="integer" column="uid" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="comment">用户id</option>
      </options>
    </field>
    <field name="addTime" type="integer" column="add_time" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">消耗爵财币的记录时间</option>
      </options>
    </field>
    <field name="prizeId" type="integer" column="prize_id" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">奖品名称</option>
      </options>
    </field>
    <field name="accountCoin" type="integer" column="account_coin" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">账户爵财币数目</option>
      </options>
    </field>
    <field name="usedCoin" type="integer" column="used_coin" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">本次消耗的爵财币数目</option>
      </options>
    </field>
    <field name="restCoin" type="integer" column="rest_coin" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="comment">账户剩余爵财币数目</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
