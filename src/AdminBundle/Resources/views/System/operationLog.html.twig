  <div class="modal-header">
	<button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
	<h4 class="modal-title">Ajax Content</h4>
</div>
<div class="modal-body">
	<!-- BEGIN PAGE CONTENT-->
	{% form_theme form 'CommonBundle:Form:bootstrap_3_horizontal_layout.html.twig' %}
	{{ form_start(form, {'action': path('/admin/form'), 'method': 'POST', 'attr': {'class' : ' form-row-sepe'}}) }}
	<div class="form-group">
	{{ form_label(form.parentTest, label|default(null),{'label_attr': {'class' : 'col-md-3'}}) }}
	{{ form_widget(form.parentTest, {'attr': {'class' : 'form-control input-xlarge select2me col-md-7 ', 'data-placeholder': '--请选择--', 'data-modules': 'dropdowns'}}) }}
	{{ form_errors(form.parentTest, {'error_attr': {'class' : 'asdfsd'}}) }}
</div>
<div class="form-group">
	{{ form_label(form.status, label|default(null),{'label_attr': {'class' : 'col-md-3'}}) }}
	{{ form_widget(form.status) }}
	{{ form_errors(form.status) }}
	</div>
	{{ form_end(form)}}
    <!-- END PAGE CONTENT-->
    </div>
<div class="modal-footer">
	<button type="button" class="btn default" data-dismiss="modal">Close</button>
	<button type="button" class="btn blue">Save changes</button>
</div>
